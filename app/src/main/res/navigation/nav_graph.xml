<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment"> <!-- 기본 시작 목적지로 설정 -->


    <fragment
        android:id="@+id/adminFragment"
        android:name="com.example.kiosk02.admin.AdminFragment"
        android:label="Admin Fragment"
        tools:layout="@layout/fragment_admin" />

    <fragment
        android:id="@+id/findEmailAdminFragment"
        android:name="com.example.kiosk02.admin.FindEmailAdminFragment"
        android:label="이메일 찾기" />
    <fragment
        android:id="@+id/findPasswordAdminFragment"
        android:name="com.example.kiosk02.admin.FindPasswordAdminFragment"
        android:label="비밀번호 찾기" />
    <fragment
        android:id="@+id/resetPasswordAdminFragment"
        android:name="com.example.kiosk02.admin.ResetPasswordAdminFragment"
        android:label="비밀번호 재설정" />
    <fragment
        android:id="@+id/adminActivity"
        android:name="com.example.kiosk02.admin.AdminActivity"
        android:label="관리자 로그인">
        <action
            android:id="@+id/action_adminActivity_to_accountFragment2"
            app:destination="@id/accountFragment" />
    </fragment>
    <fragment
        android:id="@+id/adminSignFragment"
        android:name="com.example.kiosk02.admin.AdminSignFragment"
        android:label="관리자 회원가입" />
    <fragment
        android:id="@+id/addInformActivity"
        android:name="com.example.kiosk02.admin.AddInformActivity"
        android:label="추가정보등록" />

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.kiosk02.MainFragment"
        android:label="Main Fragment"
        tools:layout="@layout/fragment_main" />

    <fragment
        android:id="@+id/showEmailAdminFragment"
        android:name="com.example.kiosk02.admin.ShowEmailAdminFragment"
        android:label="ShowEmailAdminFragment" />


    <fragment
        android:id="@+id/adminMenuListFragment"
        android:name="com.example.kiosk02.admin.menu.data.AdminMenuListFragment"
        android:label="AdminMenuListFragment"
        tools:layout="@layout/fragment_admin_menu_list"/>

    <fragment
        android:id="@+id/editMenuFragment"
        android:name="com.example.kiosk02.admin.menu.data.EditMenuFragment"
        android:label="EditMenuFragment"
        tools:layout="@layout/fragment_edit_menu">
        <argument
            android:name="menuModel"
            app:argType="com.example.kiosk02.admin.menu.data.MenuModel"
            app:nullable="true"/>
    </fragment>

    <fragment
        android:id="@+id/manageCategoriesFragment"
        android:name="com.example.kiosk02.admin.menu.data.ManageCategoriesFragment"
        android:label="ManageCategoriesFragment"
        tools:layout="@layout/fragment_manage_categories"/>

    <fragment
        android:id="@+id/menuListFragment"
        android:name="com.example.kiosk02.admin.menu.data.MenuListFragment"
